{
  "name": "06-npm-commands",
  "packageManager": "yarn@3.6.3",
  "VALUE_TO_USE": "12.10",
  "scripts": {
    "update-version": "yarn git-clone && yarn run-tsc && yarn move-files && yarn delete-files",
    "echo-value": "echo $(node -p \"require('./package.json').VALUE_TO_USE\")",
    "git-clone": "git clone --branch ...",
    "run-tsc": "tsc --project ...",
    "move-files": "cpx ...",
    "delete-files": "node ..."
  }
}
